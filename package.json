{
  "name": "fuelocr",
  "version": "1.0.0",
  "description": "Expense Tracker that utilizes Google-OCR",
  "main": "dist/server.js",
  "scripts": {
    "install": "cd client && npm install",
    "client": "cd client && npm run start",
    "prestart": "npm run tsc",
    "server": "nodemon dist/server.js",
    "tsc": "tsc --outdir dist",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node dist/server.js",
    "start:dev": "concurrently \"npm run client\" \"nodemon --ignore 'client/*'\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node server/scripts/seed.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betonit007/FuelOCR2.git"
  },
  "keywords": [
    "Google",
    "OCR"
  ],
  "author": "Tim Nagorski, Carl Vega and Jon Jackson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/betonit007/FuelOCR2/issues"
  },
  "homepage": "https://github.com/betonit007/FuelOCR2#readme",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.7.7",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.2",
    "morgan": "^1.9.1",
    "request": "^2.88.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  },
  "type": "module"
}
